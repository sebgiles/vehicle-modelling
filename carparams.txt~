t_f		1.2		# front track
t_r		1.16	# rear track
l_f		0.8112	# Front axle to CG distance
l_r		0.748	# Rear axle to CG distance
q_f		0.045	# front roll center height
q_r     0.09	# rear roll center height
h_CG    0.3		# CG height
I_f		0		# Front Steering inertia
I_r		0		# Rear steering Inertia
k_f		0		# front virtual spring stiffness
k_r		0		# rear virtual spring stiffness
b_f		0		# front virtual suspension damping 
b_r		0		# rear virtual suspension damping
m		0		# sprung mass
m_u		0		# unsprung mass
Ixx		0		# Moment of inertia about longitudinal axis (sprung only)
Iyy		0		# Moment of inertia about lateral axis (sprung only)
Izz		0		# Moment of inertia about vertical axis (sprung only)
Ixz		0		# vertical-longitudinal inertia coupling
I_u		0		# unsprung mass rotational inertia about vertical axis
r_0		0		# wheel radius
I_w		0		# wheel rotational inertia

